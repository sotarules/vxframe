{
  "env": {
    "browser": true,
    "node": true
  },
  "plugins" : [
    "react"
  ],
  "parser" : "@babel/eslint-parser",
  "parserOptions" : {
    babelOptions: {
      parserOpts: {
        plugins: ["jsx"]
      }
    },
    requireConfigFile: false,
    "ecmaFeatures": {
        "arrowFunctions": true,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": true,
        "generators": false,
        "modules": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralDuplicateProperties": false,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "spread": true,
        "superInFunctions": true,
        "templateStrings": true,
        "jsx": true
    },
  },
  "globals" : {
    "Meteor" : true,
    "DDP" : true,
    "Accounts" : true,
    "BlazeLayout" : true,
    "$" : true,
    "_" : true,
    "Templates" : true,
    "Template" : true,
    "UX" : true,
    "OLog" : true,
    "Util" : true,
    "Session" : true,
    "FlowRouter" : true,
    "PNotify" : true,
    "Sherpa" : true,
    "SherpaSubs" : true,
    "FastClick" : true,
    "TouchSpin" : true,
    "Future" : true,
    "TimezonePicker" : true,
    "SherpaState" : true,
    "ContextMaker" : true,
    "CX" : true,
    "VX" : true,
    "FX" : true,
    "moment" : true,
    "Gateways" : true,
    "Ladda" : true,
    "Tracker" : true,
    "Config" : true,
    "Tenants" : true,
    "Domains" : true,
    "Uploads" : true,
    "UploadStats" : true,
    "Notifications" : true,
    "Prospects" : true,
    "LimeLightCampaigns" : true,
    "LimeLightInstances" : true,
    "OrderMaker" : true,
    "LockClearer" : true,
    "RecordImporter" : true,
    "Reporter" : true,
    "Products" : true,
    "Cards" : true,
    "Log" : true,
    "Events" : true,
    "Binding" : true,
    "Holder" : true,
    "UXState" : true,
    "ReactiveVar" : true,
    "Blaze" : true,
    "SubsManager" : true,
    "Schema" : true,
    "SimpleSchema" : true,
    "accounting" : true,
    "amplify" : true,
    "PkgCloud" : true,
    "GatewayFetcher" : true,
    "CampaignFetcher" : true,
    "ProspectFetcher" : true,
    "ByteplantValidator" : true,
    "Mailman" : true,
    "BalanceReconciler" : true,
    "RecordRemover" : true,
    "EJSON" : true,
    "HTTP" : true,
    "Service" : true,
    "Random" : true,
    "Serv" : true,
    "Soap" : true,
    "Limelight" : true,
    "Konnektive" : true,
    "cheerio" : true,
    "ActivityStats" : true,
    "CardBalances" : true,
    "async" : true,
    "ActionRequests" : true,
    "Skills" : true,
    "References" : true,
    "CardInstances" : true,
    "Mailer" : true,
    "Practus" : true,
    "PractusState" : true,
    "EmailTemplates" : true,
    "Npm" : true,
    "BodyParser" : true,
    "Picker" : true,
    "S3" : true,
    "StripeAPI" : true,
    "Drip" : true,
    "S3Client" : true,
    "Knox" : true,
    "UserStatus" : true,
    "Observer" : true,
    "PractusSubs" : true,
    "XLSX" : true,
    "ArrayBuffer" : true,
    "Uint8Array" : true,
    "Excel" : true,
    "stream" : true,
    "EchoStream" : true,
    "Email" : true,
    "TeamStats" : true,
    "CSV" : true,
    "FilesCollection" : true,
    "SystemStats" : true,
    "Symbol" : true,
    "Set" : true,
    "Map" : true,
    "WeakMap" : true,
    "WeakSet" : true,
    "Promise" : true,
    "App" : true,
    "React" : true,
    "ReactDOM" : true,
    "Store" : true,
    "Persistor" : true,
    "SubsystemStatus" : true,
    "ProductRow" : true,
    "ProductTable" : true,
    "ReactTestUtils" : true,
    "Orders" : true,
    "OrderFetcher" : true,
    "BalanceChecker" : true,
    "Performance" : true,
    "VXApp" : true,
    "VXSubs" : true,
    "Forms" : true,
    "Composites" : true,
    "Counties" : true,
    "Functions" : true,
    "Gavel" : true,
    "FormInstances" : true,
    "WizardPages" : true,
    "Transactions" : true,
    "Mongo" : true,
    "Clipboard" : true,
    "BrowserHistory" : true,
    "Routes" : true ,
    "History" : true,
    "Attachments" : true,
    "SuperCache" : true,
    "Clients" : true,
    "KeepTrack" : true,
    "SIC" : true,
    "Carriers" : true,
    "Tasks" : true,
    "FunctionAnchors" : true,
    "Daemon" : true,
    "DaemonJobs" : true,
    "Scheduler" : true,
    "ImportSchemas" : true,
    "TaskDefinitions" : true,
    "Vendors" : true
   },
   "rules": {
/**
 * Strict mode
 */

/**
 * ES6
 */
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
/**
 * Variables
 */
    "no-shadow-restricted-names": 2, // http://eslint.org/docs/rules/no-shadow-restricted-names
    "no-unused-vars": [2, {          // http://eslint.org/docs/rules/no-unused-vars
      "vars": "local",
      "args": "after-used"
    }],
    "no-use-before-define": [2, "nofunc"],       // http://eslint.org/docs/rules/no-use-before-define

/**
 * Possible errors
 */
    "no-undef" : 2,
    "no-debugger": 1,                // http://eslint.org/docs/rules/no-debugger
    "no-alert": 1,                   // http://eslint.org/docs/rules/no-alert
    "no-constant-condition": 0,      // http://eslint.org/docs/rules/no-constant-condition
    "no-dupe-keys": 2,               // http://eslint.org/docs/rules/no-dupe-keys
    "no-duplicate-case": 2,          // http://eslint.org/docs/rules/no-duplicate-case
    "no-empty": 2,                   // http://eslint.org/docs/rules/no-empty
    "no-ex-assign": 2,               // http://eslint.org/docs/rules/no-ex-assign
    "no-extra-boolean-cast": 0,      // http://eslint.org/docs/rules/no-extra-boolean-cast
    "no-extra-semi": 2,              // http://eslint.org/docs/rules/no-extra-semi
    "no-func-assign": 2,             // http://eslint.org/docs/rules/no-func-assign
    "no-inner-declarations": 2,      // http://eslint.org/docs/rules/no-inner-declarations
    "no-invalid-regexp": 2,          // http://eslint.org/docs/rules/no-invalid-regexp
    "no-irregular-whitespace": 2,    // http://eslint.org/docs/rules/no-irregular-whitespace
    "no-obj-calls": 2,               // http://eslint.org/docs/rules/no-obj-calls
    "no-sparse-arrays": 2,           // http://eslint.org/docs/rules/no-sparse-arrays
    "no-unreachable": 2,             // http://eslint.org/docs/rules/no-unreachable
    "use-isnan": 2,                  // http://eslint.org/docs/rules/use-isnan
    "block-scoped-var": 0,           // http://eslint.org/docs/rules/block-scoped-var

/**
 * Best practices
 */
    "curly": [2, "multi-line"],      // http://eslint.org/docs/rules/curly
    "dot-notation": [2, {            // http://eslint.org/docs/rules/dot-notation
      "allowKeywords": true
    }],
    "no-caller": 2,                  // http://eslint.org/docs/rules/no-caller
    "no-else-return": 2,             // http://eslint.org/docs/rules/no-else-return
    "no-extend-native": 2,           // http://eslint.org/docs/rules/no-extend-native
    "no-extra-bind": 2,              // http://eslint.org/docs/rules/no-extra-bind
    "no-fallthrough": 2,             // http://eslint.org/docs/rules/no-fallthrough
    "no-floating-decimal": 2,        // http://eslint.org/docs/rules/no-floating-decimal
    "no-implied-eval": 2,            // http://eslint.org/docs/rules/no-implied-eval
    "no-lone-blocks": 2,             // http://eslint.org/docs/rules/no-lone-blocks
    "no-multi-str": 2,               // http://eslint.org/docs/rules/no-multi-str
    "no-native-reassign": 2,         // http://eslint.org/docs/rules/no-native-reassign
    "no-new-func": 2,                // http://eslint.org/docs/rules/no-new-func
    "no-new-wrappers": 2,            // http://eslint.org/docs/rules/no-new-wrappers
    "no-octal": 2,                   // http://eslint.org/docs/rules/no-octal
    "no-octal-escape": 2,            // http://eslint.org/docs/rules/no-octal-escape
    "no-proto": 2,                   // http://eslint.org/docs/rules/no-proto
    "no-redeclare": 2,               // http://eslint.org/docs/rules/no-redeclare
    "no-return-assign": 2,           // http://eslint.org/docs/rules/no-return-assign
    "no-script-url": 2,              // http://eslint.org/docs/rules/no-script-url
    "no-self-compare": 2,            // http://eslint.org/docs/rules/no-self-compare
    "no-sequences": 2,               // http://eslint.org/docs/rules/no-sequences
    "no-throw-literal": 2,           // http://eslint.org/docs/rules/no-throw-literal
    "no-with": 2,                    // http://eslint.org/docs/rules/no-with
    "vars-on-top": 2,                // http://eslint.org/docs/rules/vars-on-top
    "wrap-iife": [2, "any"],         // http://eslint.org/docs/rules/wrap-iife
    "yoda": 2,                       // http://eslint.org/docs/rules/yoda
    "max-len": [2, 1000, 2],         // http://eslint.org/docs/rules/max-len

/**
 * Style
 */
    "indent": [2, 4],                // http://eslint.org/docs/rules/indent
    "brace-style": [2,               // http://eslint.org/docs/rules/brace-style
      "stroustrup", {
      "allowSingleLine": true
    }],
    "quotes": [
      2, "double", "avoid-escape"    // http://eslint.org/docs/rules/quotes
    ],
    "comma-spacing": [2, {           // http://eslint.org/docs/rules/comma-spacing
      "before": false,
      "after": true
    }],
    "comma-style": [2, "last"],      // http://eslint.org/docs/rules/comma-style
    "eol-last": 2,                   // http://eslint.org/docs/rules/eol-last
    "func-style": [2, "declaration", { "allowArrowFunctions" : true }], // http://eslint.org/docs/rules/func-style
    "no-multiple-empty-lines": [2, { // http://eslint.org/docs/rules/no-multiple-empty-lines
      "max": 2
    }],
    "no-nested-ternary": 2,          // http://eslint.org/docs/rules/no-nested-ternary
    "no-new-object": 2,              // http://eslint.org/docs/rules/no-new-object
    "no-array-constructor": 2,       // http://eslint.org/docs/rules/no-array-constructor
    "no-spaced-func": 2,             // http://eslint.org/docs/rules/no-spaced-func
    "no-trailing-spaces": 2,         // http://eslint.org/docs/rules/no-trailing-spaces
    "no-underscore-dangle": 0,       // http://eslint.org/docs/rules/no-underscore-dangle
    "semi-spacing": [2, {            // http://eslint.org/docs/rules/semi-spacing
      "before": false,
      "after": true
    }],
    "space-before-blocks": 2,        // http://eslint.org/docs/rules/space-before-blocks
    "space-before-function-paren": [2, "never"], // http://eslint.org/docs/rules/space-before-function-paren
    "space-infix-ops": 2           // http://eslint.org/docs/rules/space-infix-ops
  }
}